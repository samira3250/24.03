#include <stdio.h>

int main() {
  int a, b, c;
  printf("Введите три числа (через пробел): ");
  scanf("%d %d %d", &a, &b, &c);

  int negativeCount = (a < 0) + (b < 0) + (c < 0); 
  int sign = (a == 0 || b == 0 || c == 0) ? 0 : ((negativeCount % 2 == 0) ? 1 : -1);

  printf("%d\n", sign);
  return 0;
}

