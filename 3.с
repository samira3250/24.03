#include <stdio.h>

int main() {
  // 1. Объявление переменных
  int a, b, c; // Объявляем три целочисленные переменные: a, b, c.

  // 2. Запрос ввода
  printf("Введите три числа (через пробел): "); // Выводим сообщение с просьбой ввести три числа через пробел.

  // 3. Считывание ввода
  scanf("%d %d %d", &a, &b, &c); // Считываем три целых числа, введенных пользователем, и сохраняем их в переменные a, b и c.

  // 4. Подсчет отрицательных чисел
  int negativeCount = 0; // Инициализируем счетчик отрицательных чисел значением 0.

  // 4.1 Проверка числа 'a'
  if (a < 0) { // Если число 'a' меньше 0 (отрицательное)...
    negativeCount = negativeCount + 1; // ...увеличиваем счетчик 'negativeCount' на 1.
  }

  // 4.2 Проверка числа 'b'
  if (b < 0) { // Если число 'b' меньше 0 (отрицательное)...
    negativeCount = negativeCount + 1; // ...увеличиваем счетчик 'negativeCount' на 1.
  }

  // 4.3 Проверка числа 'c'
  if (c < 0) { // Если число 'c' меньше 0 (отрицательное)...
    negativeCount = negativeCount + 1; // ...увеличиваем счетчик 'negativeCount' на 1.
  }

  // 5. Определение знака произведения
  int sign; // Объявляем переменную 'sign' для хранения знака произведения.

  // 5.1 Проверка на наличие нуля
  if (a == 0 || b == 0 || c == 0) { // Если хотя бы одно из чисел a, b или c равно 0...
    sign = 0; // ...знак произведения равен 0.
  } else { // Иначе (если все числа не равны 0)...
    // 5.2 Определение знака по количеству отрицательных чисел
    if (negativeCount % 2 == 0) { // Если количество отрицательных чисел четное...
      sign = 1; // ...знак произведения положительный (1).
    } else { // Иначе (если количество отрицательных чисел нечетное)...
      sign = -1; // ...знак произведения отрицательный (-1).
    }
  }

  // 6. Вывод результата
  printf("%d\n", sign); // Выводим значение переменной 'sign' (знак произведения) на экран.
  // '\n' в конце строки добавляет символ новой строки.  Это заставляет следующий вывод в консоли начинаться с новой строки, а не сразу после текущего.
  // Это улучшает читаемость вывода.
  return 0; // Указываем, что программа завершилась успешно.
}
